\set QUIET 1

\pset border 2
\pset linestyle unicode
\pset null NULL

\setenv PAGER pspg
\pset pager on

\set PROMPT1 '%n@%M:%> [%x%/]> '
\set HISTCONTROL ignoredups
\set VERBOSITY verbose

-- :desc 'table';
\set desc 'select column_name, data_type, is_nullable, column_default, is_generated, generation_expression from information_schema.columns where table_name = '

\timing on

\unset QUIET
