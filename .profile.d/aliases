#! /usr/bin/env bash

alias dt="diffstat -C"
alias g="links https://www.google.com/"
alias grep="grep --color=auto"
alias h="history"
alias https="http --verify=no"
alias j="jobs -l"
alias l="ls --color=auto -ap"
alias ll="ls --color=auto -alp"
alias maxima="rlwrap maxima"
alias p="procs"
alias psql="pgcli"
alias rcli="redis-cli"
alias sbcl="rlwrap sbcl"
alias sc="shellcheck -f gcc"
alias show="open -a Preview"
alias ssh="env -u COLORTERM TERM=xterm-256color ssh"
alias t="tree"
